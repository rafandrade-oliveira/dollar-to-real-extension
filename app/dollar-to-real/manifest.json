{
  "name": "Dollar to Real",
  "version": "1",
  "description": "Dollar to Real - Conversor",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Dollar to Real - Conversor",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_page_action": {
      "description": "Copy the link to the active pipeline",
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "MacCtrl+I"
      }
    }
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
